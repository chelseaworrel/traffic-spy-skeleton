<h1><%= @identifier %></h1>

<br>

<h2>Application Details</h2>
<br>

<h3>Web Browser Breakdown Across Requests</h3>
<table id="browser_details">
  <th>Browser</th>
  <th>Count</th>
  <% @counted_browsers.each do |browser, count| %>
  <tr>
    <td><%= browser %></td>
    <td><%= count %></td>
  </tr>
  <% end %>
</table>

<h3>Operating System Breakdown Across Requests</h3>
<table id="os_details">
  <th>Operating System</th>
  <th>Count</th>
  <% @counted_os.each do |os, count| %>
  <tr>
    <td><%= os %></td>
    <td><%= count %></td>
  </tr>
  <% end %>
</table>

<h3>Screen Resolutions Across Requests</h3>
<table id="resolution_details">
  <th>Screen Resolutions</th>
  <% @resolutions.each do |resolution| %>
  <tr>
    <td><%= resolution %></td>
  </tr>
  <% end %>
</table>

<h3>Most Visited URLs</h3>
<table id="sorted_urls">
  <th>URL</th>
  <th>Total Times Visited</th>
  <% @counted_urls.each do |url, times_visited| %>
    <tr>
      <td><%= url %></td>
      <td><%= times_visited %></td>
    </tr>
  <% end %>
</table>

<h3>Average response times</h3>
<table id="response_times">
  <th>URL</th>
  <th>Time</th>
</table>
